public class Barcodes {
    int id;
    String barcode;
    int product_id;

    public Barcodes() {
    }

    public Barcodes(String barcode, int product_id) {
        this.barcode = barcode;
        this.product_id = product_id;
    }

    public Barcodes(int id, String barcode, int product_id) {
        this.id = id;
        this.barcode = barcode;
        this.product_id = product_id;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getBarcode() {
        return barcode;
    }

    public void setBarcode(String barcode) {
        this.barcode = barcode;
    }

    public int getProduct_id() {
        return product_id;
    }

    public void setProduct_id(int product_id) {
        this.product_id = product_id;
    }
}
